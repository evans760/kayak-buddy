<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<%= link_to('logout', logout_path, method: :delete) %>

<% @users.each do |user| %>
<div>
  <div><%= user.name %></div>
  <div><%= user.gender %></div>
  <div><%= user.skill_level %></div>
  <div><%= user.city %></div>
  <div><%= user.more_info %></div>
</div>
<% end %>